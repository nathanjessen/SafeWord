{
  "name": "safeword",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@heroicons/react": "^2.0.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^3.0.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
